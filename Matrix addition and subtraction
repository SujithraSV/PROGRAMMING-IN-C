#include <stdio.h>
// add and sub 3*3 
int main()
{
    
    int a[3][3],b[3][3],c[3][3];
    
    printf("enter the array 1 elements ");
    for(int i=0;i<3;i++)
    {
        for(int j=0;j<3;j++)
        {
            scanf("%d",&a[i][j]);
        }
    } 
    printf("enter the array 2 elements ");
    for(int i=0;i<3;i++)
    {
        for(int j=0;j<3;j++)
        {
            scanf("%d",&b[i][j]);
        }
    }
    //add
    for(int i=0;i<3;i++)
    {
        for(int j=0;j<3;j++)
        {
            c[i][j]=a[i][j]+b[i][j];
        }
    } 
    printf("after addition:\n");
    for(int i=0;i<3;i++)
    {
        for(int j=0;j<3;j++)
        {
            printf("%d \t",c[i][j]);
        }
        printf("\n");
    } 
    //sub
    for(int i=0;i<3;i++)
    {
        for(int j=0;j<3;j++)
        {
            c[i][j]=a[i][j]-b[i][j];
        }
    } 
    printf("after subtraction:\n");
    for(int i=0;i<3;i++)
    {
        for(int j=0;j<3;j++)
        {
            printf("%d \t",c[i][j]);
        }
        printf("\n");
    } 
    

    return 0;
}
